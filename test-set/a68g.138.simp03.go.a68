#simp03#
BEGIN #Referencing and dereferencing#
      INT i1 = 1;
      
      INT i2 = 2;
      
      INT i3 = 3;
      
      INT ii1 := i1;
      
      INT ii2 := i2;
      
      REF INT iii1 := ii1;
      
      print ((newline, "Value should be 1", iii1));
      iii1 := ii2;
      print ((newline, "Value should be 2", iii1))
END