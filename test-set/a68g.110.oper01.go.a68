#oper01#
BEGIN # Operator test #
      OP +:= = (INT a, b) INT: a + b;
      
      OP +:= = (INT a, REAL b) INT: ROUND (a - b);
      
      print (2 +:= 1);
      # yields 3 # print (2 +:= 1.0)
# yields 1 #
END