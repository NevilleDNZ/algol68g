PR quiet PR

#simp02#
range_of_variables:
BEGIN #Test that ranges are correct#
      print ((newline, "Values are 2,5,3,4", newline));
      INT i, j;
      
      i := 3;
      j := 4;
      BEGIN INT i, k;
            
            i := 2;
            k := 5;
            print ((i, k))
      END;
      print ((i, j))
END