PR quiet PR

#decl05#
BEGIN # Application of a virtually useless mode #
      print (("Result should be: ppp", newline, newline));
      MODE P = PROC (P) P;
      
      P p = (P p) P:
      (print ("p");
       p);
      
      (p (p (p (p))))
END