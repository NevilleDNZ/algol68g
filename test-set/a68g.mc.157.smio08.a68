PR quiet PR

#smio08#
BEGIN # 10/08/73, R van Vliet; 30/09/75, revised.
      Test the print and putroutines.#
      print ((newline, "It should print 2:"));
      ([1 : 7] PROC (REF FILE) VOID p;
       
       INT k := 0;
       
       p[1] := (REF FILE f) VOID:
       (k +:= 1; SKIP);
       FOR i FROM 2 TO UPB p 
       DO p[i] := (REF FILE f) VOID:
          (k +:= 1; GOTO l)
       OD;
       print ((REF FILE f) VOID:
              FOR i TO UPB p 
              DO p[i] (f)
              OD);
       l:
       print ((k, new line)))
END