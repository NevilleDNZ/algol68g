PR quiet PR

#mdeq04#
BEGIN # Unions #
      MODE N = UNION (REAL, UNION (BOOL, INT)),
            M = UNION (UNION (REAL, BOOL), INT);
      
      PROC M (PROC N (SKIP));
      PROC N (PROC M (SKIP));
      # both okay, since 'M' and 'N' are the same #
      MODE U = UNION (INT, PROC (U) INT),
            V = UNION (U, PROC (V) INT);
      
      PROC U (PROC V (SKIP));
      PROC V (PROC U (SKIP));
      # both okay, since 'U' and 'V' are the same #
      SKIP
END