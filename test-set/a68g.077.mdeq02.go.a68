#mdeq02#
BEGIN # Mode equivalencing #
      MODE N = PROC (M) M,
            M = PROC (N) N;
      
      PROC M (PROC N (SKIP));
      PROC N (PROC M (SKIP));
      # Both okay, since 'M' and 'N' are the same #
      SKIP
END