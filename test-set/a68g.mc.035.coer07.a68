PR quiet PR

#coer07#
BEGIN # Weak balance # print ((COMPL x := 1;
                               
                               CASE 2
                               IN NIL, IF [] BOOL (TRUE, FALSE)[2]
                                  THEN REF REF [] COMPL: NIL
                                  ELSE x
                                  FI, LOC PROC REF [] STRUCT (REAL re, im)
                               ESAC
                               # REF [] COMPL = x #
                               [1] := 3;
                               x))
#  3.0 I 0.0 #
END