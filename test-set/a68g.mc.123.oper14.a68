PR quiet PR

#oper14#
BEGIN print (("Results must be:", newline, 4, 5, 5, 4, newline, 1, 2, 2, newline, 1, 1, 1, newline, 2, 1, 3, newline, 1, newline, 1, 1, 1, 1, newline, newline, "Results are:", newline));
      print ((UPB [] INT (1, 2, 3, 4), UPB "abcde", UPB [] INT (SKIP, SKIP)[1 : 1@5], 2 UPB [, ] INT (1, 2)[, @4]));
      print (newline);
      # All declarers are of the mode row-of, so UPB/LWB should work #
      print ((UPB UNION ([] INT, [, ] INT) ([] INT (1)), UPB UNION ([] INT, [, ] INT) ([, ] INT (1, 1)), UPB UNION ([] STRING, STRING) ("ab")));
      print (newline);
      print ((LWB UNION ([] INT, [, ] INT) ([] INT (1)), LWB UNION ([] INT, [, ] INT) ([, ] INT (1, 1)), LWB UNION ([] STRING, STRING) ("ab")));
      print (newline);
      FOR i TO 3 
      DO print (i UPB [, , ] CHAR ("abc", "def"))
      OD;
      print (newline);
      print (LWB LOC STRING LWB LOC STRING);
      print (newline);
      # Balance #
      FOR n TO 4 
      DO print (n UPB CASE n
                IN [] INT (1), [, ] REAL (1), UNION ([] INT, [, , ] BOOL) ([, , ] BOOL (TRUE)), UNION ([] INT, UNION ([, ] REAL, [, , , ] CHAR)) ([, , , ] CHAR ("a"))
                ESAC)
      OD
END