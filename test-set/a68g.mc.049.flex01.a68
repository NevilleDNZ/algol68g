  PR quiet PR

#flex01#
( # OK #
    MODE S = FLEX [1 : 0] CHAR, T = [1 : 0] CHAR;
    REF STRING n = LOC S:= "Next line will be empty, then a";
    UNION(REF S, REF T) f = LOC T := "";
    UNION(STRING, CHAR) u = UNION(S, T, CHAR) ("a");
    print((n, newline));
    print(((f | (REF S s):s , (REF T t):t), newline));
    print((u, newline))
)
