#simp05#

# Simple jumps #
(INT j := 0, i;
 
 k: i := j;
 IF i >= 2
 THEN GOTO l
 FI;
 print ("0");
 m:
 IF i >= 1
 THEN n
 FI;
 print ("0");
 o: GOTO p;
 l: print ("1");
 i := i - 2;
 m;
 n: print ("1");
 o;
 p: print (newline);
 j := j + 1;
 IF j <= 3
 THEN k
 FI
# Result:  00
             01
             10
             11 #
)