#stow02#
BEGIN # Some slices #
      [0 : 7] [0 : 15] INT a;
      
      INT n := 0;
      
      FOR i TO 8 
      DO FOR j TO 16 
         DO a[i - 1][j - 1] := n +:= 1
         OD
      OD;
      print (a[0][15]);
      #16# print (a[0 : 0 AT 0][0][15]);
      #16# print (a[0 : 0 AT 0][0 : 0 AT 0][0][3 : 15][11 : 13 AT 2][4]);
      #16# print (newline);
      [] INT k = a[0 : 0][15]
      # wrong, a[0:0] has bounds [1:1][0:15],
                so there occurs overflow #
      ;
      
      SKIP
END