PR quiet PR

#garb02#
BEGIN # Test garbage collector #
      REF [] REAL x, y,
      INT n := 0;
      
      l: x := HEAP [1 : 1000] REAL;
      y := HEAP [1 : 1000] REAL;
      IF (n +:= 1) < 1000
      THEN l
      FI;
      print (("collections, garbage, collect seconds:", newline));
      print ((collections, garbage, 
              # collect seconds, #
              newline))
END