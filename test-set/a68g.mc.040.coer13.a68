PR quiet PR

#coer13#
BEGIN # Soft balance with EXIT's #
      INT i;
      
      [1 : 1] INT ri, rj;
      
      PROC pri = REF [] INT: rj;
      
      FOR c TO 3 
      DO ([] PROC VOID switch = (lrri, li, lpri);
          
          switch[c];
          SKIP # hip # EXIT
          lrri: LOC REF [] INT := ri # deref # EXIT
          li: i # row # EXIT
          lpri: pri
         # deproc #
         ) := c
      OD;
      print ((ri, i, pri, newline))
# 1 2 3 #
END