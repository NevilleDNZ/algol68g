  PR quiet PR

#coer02#
BEGIN # Widening #
  FOR i TO 2 DO
  print(
    CASE i IN TRUE,     2r1    OUT []BOOL(TRUE) ESAC
   [CASE i IN  1  , bits_width OUT     SKIP     ESAC]
    ) OD;                                    # TT #
  print(newline);
  FOR n TO 3 DO
  print((re OF CASE n IN 1, 2.0, 3 I 5 ESAC,
         im OF CASE n IN 1, 2.0, 3 I 5 ESAC))
  OD
                                       # 1.0 0.0, 2.0 0.0, 3.0 5.0 #
END
