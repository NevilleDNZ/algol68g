PR quiet PR

#jump04#
(# Jump out of procedure #
 (# directly #
  PROC jump = VOID:
       (print (2);
        print ((9, GOTO l, 8)));
  
  print (1);
  jump;
  print (7);
  l: print (3));
 (# indirectly #
  MODE HIDE = PROC VOID;
  
  HIDE p = (TRUE | GOTO m);
  
  print (4);
  p;
  print (6);
  m: print (5)))
# result is 1, 2, 3, 4, 5 #
