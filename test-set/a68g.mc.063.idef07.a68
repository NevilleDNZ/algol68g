PR quiet PR

#idef07#
BEGIN # Redeclaring LWB #
      OP LWB = ([] INT a) REAL:
         a[1] + a[2];
      
      OP LWB = ([] REAL a) REAL:
         a[1] - a[2];
      
      print (LWB (1 | (8, 2), 3, [] INT: SKIP));
      # 10.0 # print (LWB (1 | (8, 2), 3, [] REAL: SKIP));
      #  6.0 # SKIP
END