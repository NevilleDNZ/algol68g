PR quiet PR

#idrl01#
BEGIN # Identity relations #
      REAL x;
      
      REF REAL y := x;
      
      print ((x :=: y, y :=: x, newline)) # TRUE, TRUE # ;
      print ((x :=: REF [] REAL (x)[1], newline)) # TRUE # ;
      print ((x :=: REF [] REAL (x), newline))
# FALSE #
END