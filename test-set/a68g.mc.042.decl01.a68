#decl01#
BEGIN # Some declarers #
  [1:10] INT i,
  [1:10] STRUCT (REF [] INT i, BOOL j) k,
  [1:10] STRUCT([1:10] INT i, BOOLj)l,
  [1:10] REF [] INT p,
   # formal, so no bounds allowed: #
