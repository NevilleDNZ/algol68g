PR quiet PR

#misc04#
BEGIN # A primary #
      MODE # M = PROC(INT)[]PROC P, #
           P = REF [] PROC (CHAR) N,
            N = [1 : 0] UNION (INT, CHAR);
      
      INT ii
      # partial parameter #
      ;
      
      FOR k TO 5 
      DO FOR l TO 5 
         DO FOR m TO 2 
            DO print (CASE BEGIN (INT i) [] PROC P:
                                 (ii := i;
                                  P:
                                  HEAP [1 : 2] PROC (CHAR) N := (SKIP, (CHAR c) N:
                                                                 (ii, c)))
                           END (k)[1][2] ("abcde"[l])[m]
                      IN (CHAR c): c, (INT i): "12345"[i]
                      ESAC)
            OD
         OD
      OD
# hit 'm hard, the output should be :
    1a1b1c1d1e2a2b ...   5c5d5e #
END