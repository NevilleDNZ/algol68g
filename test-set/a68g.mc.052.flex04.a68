PR quiet PR

#flex04#
(print (("Ghost element test. Results should be:", newline, 3, "  3", newline, "followed by error in multiple assignation", newline, newline, "Results are:", newline));
 FOR k FROM 3 TO 4 
 DO FLEX [1 : 0] [1 : 3] CHAR flex fix;
    
    flex fix := "  34"[ : k];
    print (k);
    print (flex fix);
    print (newline)
 OD)