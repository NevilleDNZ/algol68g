 # KO #
 );
 (OP LWB = (REF [] INT i) INT: 1; LWB [] REAL (1)
 # KO #
 );
 (OP UPB = (REF UNION ([] INT, [] BOOL) i) INT: 1;
  
  UPB [] REAL (1)
 # KO #
 );
 (OP LWB = (REF UNION (REF [] INT, [] BOOL) i) INT: 1;
  
  LWB [] REAL (1)
 # OK #
