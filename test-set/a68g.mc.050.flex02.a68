  PR quiet PR

#flex02#
BEGIN # Transiency tests, all OK #
  BOOL b = TRUE, y = FALSE;
  print((
   IF b THEN LOC CHAR ELSE (LOC STRING)[1] FI:= "a",
        newline));
  print((
   IF b THEN LOC[1:3]CHAR ELSE (LOC STRING)[] FI:= "bcd",
        newline));
  print((
   IF y THEN LOC STRING ELSE LOC[1:1,1:3]CHAR FI:= "efg",
        newline))
                      #  a
                         bcd
                         efg  #
END
