#oper05#
BEGIN # Operator test, mutual recursion #
      PRIO +> = 1, +< = 1;
      
      OP +> = (INT a, b) INT: a +< b;
      
      OP +< = (INT a, b) INT: a +> b;
      
      1 +> 2
# loop #
END