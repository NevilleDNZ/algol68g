#oper02#
BEGIN # Operator #
      OP + = (UNION (INT, BOOL) a) INT:
         (a | (BOOL): 1, (INT): 2);
      
      print (+IF TRUE
             THEN TRUE
             ELSE 0
             FI);
      # 1 # print (+IF FALSE
                   THEN TRUE
                   ELSE 0
                   FI)
# 2 #
END